{
	"schemaVersion": 1,
	"id": "wild_temperature",
	"version": "${version}",
	"name": "Wild Temperatures",
	"description": "a temperature mod for minecraft that tries to use vanilla concepts. no ice packs :)",
	"authors": [
		"pikachurro"
	],
	"contact": {
		"homepage": "https://pikachurro.xyz/#minecraft",
		"sources": "https://github.com/pikachurr0/WildTemperatures",
		"issues" : "https://github.com/pikachurr0/WildTemperatures/issues"
	},
	"license": "ARR",
	"icon": "assets/wild_temperature/icon.png",
